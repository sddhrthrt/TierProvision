{
    "Job": {
        "ID": "example",
        "Name": "example",
        "Type": "service",
        "Priority": 50,
				"Region": "global",
        "Datacenters": [
            "dc1"
        ],
        "TaskGroups": [{
            "Name": "template",
            "Count": 1,
            "Tasks": [{
                "Name": "sampleTask",
                "Driver": "docker",
                "Config": {
                    "image": "sampleImage"
                },
                "Resources": {
                    "CPU": 500,
                    "MemoryMB": 256,
                    "Networks": [{
                        "MBits": 1000,
                        "DynamicPorts": [{
                            "Label": "http",
                            "Value": 0
                        }]
                    }]
                },
                "Leader": false
            }],
            "RestartPolicy": {
                "Interval": 300000000000,
                "Attempts": 10,
                "Delay": 25000000000,
                "Mode": "delay"
            },
            "EphemeralDisk": {
                "SizeMB": 300
            }
        }],
        "Update": {
            "MaxParallel": 1,
            "MinHealthyTime": 10000000000,
            "HealthyDeadline": 180000000000,
            "AutoRevert": false,
            "Canary": 0
        }
    }
}
